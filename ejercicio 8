#include <iostream>
#include<math.h>

using namespace std;

int main()
{
    float a, b, c, d, e, f, g, z;
    cout<<"ingresa a: ";
	cin>>a;
	cout<<"ingresa b: ";
	cin>>b;
	cout<<"ingresa c: ";
	cin>>c;
	cout<<"ingresa d: ";
	cin>>d;
	cout<<"ingresa e: ";
	cin>>e;
	cout<<"ingresa f: ";
	cin>>f;
	
	g=((c*e)-(b*f))/((a*e)-(b*d));
	z=((f*a)-(c*d))/((a*e)-(b*d));
	cout<<"El valor de x es: "<<g<<endl;
	cout<<"El valor de y es: "<<z<<endl; 

    return 0;
}
