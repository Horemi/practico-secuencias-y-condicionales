#include <iostream>

using namespace std;

int main()
{
    int num1, num2;
    cout<<"ingrese numero: ";
    cin>>num1;
    cout<<"ingrese numero: ";
    cin>>num2;
    if(num1==num2){
        cout<<"los numeros son iguales";
    }
    else{
        if (num1>num2){
        cout<<"el mayor numero es: "<<num1;
        }
        else {
        cout<<"el mayor numero es: "<<num2;
         }

    }
    return 0;
}
